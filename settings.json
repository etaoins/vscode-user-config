{
  "workbench.statusBar.feedback.visible": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "workbench.fontAliasing": "auto",
  "git.autofetch": true,
  "vim.textwidth": 99,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["]", "c"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.editor.nextChange",
          "args": []
        }
      ]
    },
    {
      "before": ["[", "c"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.editor.previousChange",
          "args": []
        }
      ]
    }
  ],
  "arret.trace.server": "verbose",
  "arret.builtRepository": "/Users/ryan/Code/arret",
  "[arret]": {
    "editor.tabSize": 2,
    "editor.detectIndentation": false
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.detectIndentation": false
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.detectIndentation": false
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.autoImportSuggestions.enabled": false,
  "editor.glyphMargin": false,
  "cSpell.diagnosticLevel": "Hint",
  "spellright.language": ["en-AU"],
  "spellright.groupDictionaries": false,
  "spellright.documentTypes": ["markdown", "mdx", "latex", "plaintext", "rust"],
  "spellright.notificationClass": "hint",
  "spellright.ignoreRegExpsByClass": {
    "rust": ["/`[^`]*`/g"]
  },
  "rust-analyzer": {
    "inlayHints": {
      "typeHints": false,
      "parameterHints": false
    },
    "checkOnSave": {
      "command": "clippy"
    },
    "serverPath": "rust-analyzer"
  },
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingOvertype": "never",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": false
}